{
  "name": "@okta/openapi",
  "version": "0.10.0",
  "description": "Utilities to generate OpenAPI specifications for the Okta API",
  "main": "./dist/spec.json",
  "bin": {
    "okta-sdk-generator": "lib/generator.js"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "node lib/build.js",
    "test": "npm run test:unit && npm run test:coverage",
    "test:unit": "istanbul cover _mocha -- \"test/**/*.js\"",
    "test:coverage": "istanbul check-coverage --statements 98 --branches 100 --functions 100 --lines 98",
    "scrape:clean": "python resources/docs/scraper.py clean",
    "prescrape": "bash resources/docs/python_setup.sh",
    "scrape": "python resources/docs/scraper.py"
  },
